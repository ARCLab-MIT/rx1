# Joint position controllers configuration with PID

joint_state_controller:
  type: joint_state_controller/JointStateController
  publish_rate: 50

controller_manager:
  # Base to torso yaw joint controller
  base2torso_yaw_joint_position_controller:
    type: position_controllers/JointPositionController
    joint: base2torso_yaw_joint
    pid:
      p: 100.0
      i: 0.01
      d: 10.0

  # Torso yaw to pitch joint controller
  torso_yaw2pitch_joint_position_controller:
    type: position_controllers/JointPositionController
